{
  "rewrites": [
    { 
      "source": "/static/(.*)", 
      "destination": "/static/$1" 
    },
    { 
      "source": "/favicon.ico", 
      "destination": "/favicon.ico" 
    },
    { 
      "source": "/robots.txt", 
      "destination": "/robots.txt" 
    },
    { 
      "source": "/sitemap.xml", 
      "destination": "/sitemap.xml" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/index.html" 
    }
  ]
}
